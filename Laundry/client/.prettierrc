{
  "semi": false,
  "arrowParens": "always",
  "singleQuote": true,
  "vue/html-closing-bracket-newline":false,
  "break_chained_methods": false,
  "brace_style": "none",
  "end_with_newline": false,
  "indent_char": "",
  "indent_inner_html": false,
  "indent_scripts": "keep",
  "indent_size": 2,
  "max_preserve_newlines": 0,
  "preserve_newlines": false,
  "unformatted": ["a", "img", "code", "pre", "sub", "sup", "em", "b", "i", "u", "strike", "big", "pre", "h1", "h2", "h3", "h4", "h5", "h6"],
  "wrap_line_length": 0,
}
